import tensorflow as tf


acc = tf.Variable(1, name="Acc")
vl = tf.Variable(1, name="val")

fct = acc + vl

update_op = tf.assign(acc, fct)

# init all variables
init = tf.global_variables_initializer()

# computation graph is finished, now obtain a session
with tf.Session() as sess:
    sess.run(init) # init
    for vlval in range(5):
        print(vlval)
        sess.run(update_op)
        print(fct.eval())



